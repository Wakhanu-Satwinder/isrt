{
      "version": 2,
      "builds": [
        {
          "src": "sts/wsgi.py",
          "use": "@vercel/python",
          "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        }
      ],
      "rewrites": [
        {
          "src": "/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "/(.*)",
          "dest": "sts/wsgi.py"
        },
         {
          "src": "/[^.]+",
          "dest": "/index.htmlerror: src refspec master does not match any"
        }
]
}
    
    
    